{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/about",
        "dest": "/about"
      },
        {
        "src": "/experience",
        "dest": "/experience"
      },
    {
        "src": "/projects",
        "dest": "/projects"
      },
      {
        "src": "/(.*)",
        "dest": "/index"
      }
    ]
  }